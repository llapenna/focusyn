    "@visx/tooltip": "^3.3.0",
    "@visx/zoom": "^3.3.0",
    "active-win": "^8.1.1",
    "chart.js": "^4.4.0",
    "framer-motion": "^10.18.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
